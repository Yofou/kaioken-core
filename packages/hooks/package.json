  "devDependencies": {
    "kaioken": "^0.13.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-kaioken": "^0.1.2"
  }
}
